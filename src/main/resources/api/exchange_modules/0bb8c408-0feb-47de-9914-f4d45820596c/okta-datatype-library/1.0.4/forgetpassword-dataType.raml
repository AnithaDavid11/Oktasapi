#%RAML 1.0 DataType
type: object
displayName: "Forgot password with email"
properties:
  username:
   type: string
   required: true
   displayName: "username"
   example: "harshaap@email.com"
  factorType:
    type: string
    required: true
    displayName: "factorType"
    example: "EMAIL"
  relayState:
    type: string
    required: true
    displayName: "relayState"
    example: "/myapp/some/deep/link/i/want/to/return/to"